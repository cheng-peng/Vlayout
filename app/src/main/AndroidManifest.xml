<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cxp.vlayout">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.LinearLayoutHelperActivity"/>
        <activity android:name=".ui.GridLayoutHelperActivity"/>
        <activity android:name=".ui.StaggeredGridLayoutHelperActivity"/>
        <activity android:name=".ui.FixLayoutHelperActivity"/>
        <activity android:name=".ui.ScrollFixLayoutHelperActivity"/>
        <activity android:name=".ui.ColumnLayoutHelperActivity"/>
        <activity android:name=".ui.SingleLayoutHelperActivity"/>
        <activity android:name=".ui.OnePlusNLayoutHelperActivity"/>
        <activity android:name=".ui.FloatLayoutHelperActivity"/>
        <activity android:name=".ui.StickyLayoutHelperActivity"/>
        <activity android:name=".ui.AllActivity"/>
    </application>

</manifest>